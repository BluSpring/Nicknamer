{
  "schemaVersion": 1,
  "id": "nicknamer",
  "version": "${version}",
  "name": "Nicknamer",
  "description": "Client-side mod for you to add nicknames and pronouns to pretty much any player.",
  "authors": [
    "BluSpring"
  ],
  "contact": {
    "website": "https://bluspring.xyz"
  },
  "license": "MIT",
  "icon": "assets/nicknamer/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "xyz.bluspring.nicknamer.client.NicknamerClient"
    ],
    "main": [
      "xyz.bluspring.nicknamer.Nicknamer"
    ]
  },
  "mixins": [
    "Nicknamer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric": "*",
    "minecraft": "1.18.1",
    "fabric-language-kotlin": "*"
  }
}
